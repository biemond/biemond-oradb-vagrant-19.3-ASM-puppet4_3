<% if @oraPassword -%>
  connect <%= @oraUser -%>/<%= @oraPassword -%>;
<% else -%>
  connect / as <%= @oraUser -%>;
<% end %>
SET PAGES 0 EMB ON NEWP NONE
SET NEWPAGE 0
SET SPACE 0
SET LINESIZE 32767
SET ECHO OFF
SET FEEDBACK OFF
SET VERIFY OFF
SET HEADING ON
SET MARKUP HTML OFF SPOOL OFF
SET COLSEP ','
SPOOL <%= output_file %>
<%= command %>;
spool off
